void main() {
	Student student;
	Fruit fruit;
	Person person;

	student = New(Student);
	fruit = New(Fruit);
	person = New(Person); 	
}

void println(string s1, string s2) {
	Print(s1, "\n");
	Print(s2, "\n");
	return;
}

int generateNumber() {
	return ReadInteger();
}

class Student extends Person {
	string school;
	string major;
	void printDetail() {
		Print(name, "\n");
		Print(major, "\n");
	}
	string getName() { return "need appropriate permission"; }
}

class Fruit {
	string name;
	int price;
	bool tasty;
}

class Person {
	string name;
	int age;
	int getAge() { return age; }
	string getName() { return name; }
}
