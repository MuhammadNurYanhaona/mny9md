void main() {
	int x;
	int[] y;
	y = NewArray(10, int);
	println("Hello World", "From Charlottesville");
	x = generateNumber();
	Print("Number is ", x, "\n");
	Print("Array size is ", y.length());
}

void println(string s1, string s2) {
	Print(s1, "\n");
	Print(s2, "\n");
	return;
}

int generateNumber() {
	return ReadInteger();
}

class Student extends Person {
	string school;
	string major;
	void printDetail() {
		Print(name, "\n");
		Print(major, "\n");
	}
}

class Fruit {
	string name;
	int price;
	bool tasty;
}

class Person {
	string name;
	int age;
}
